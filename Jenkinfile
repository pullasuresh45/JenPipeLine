pipeline {
    agent { label 'MyNode01' }
    stages {
        stage("git"){
            steps{
            git 'https://github.com/pullasuresh45/MavenProject.git'
        }
        }
        stage("build"){
            steps{
            sh "mvn clean install"
                 }
        }
        stage("Verify Versions"){
            steps{
            sh ''' 
                java -version
                mvn -version
                '''
                 }
        }
    }
}